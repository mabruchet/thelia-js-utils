{
    "compilerOptions": {
        "paths": {
            "https://esm.sh/nanoid": ["./node_modules/nanoid"],
            "https://esm.sh/*": ["./node_modules/@types/*"]
        },
      "target": "esnext",
      "module": "esnext",
      "jsx": "react",
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
    },
    "exclude": ["node_modules"]
  }
